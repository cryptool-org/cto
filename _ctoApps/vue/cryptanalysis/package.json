{
  "name": "AES-Cryptanalysis",
  "version": "1.0.0",
  "description": "AES Cryptanalysis",
  "private": true,
  "license": "Unlicense",
  "files": [
    "dist"
  ],
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "http-server -o -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "browserify": {
    "transform": [
      "vueify",
      ["babelify", {"presets": ["es2015"]}]
    ]
  },
  "browser": {
    "vue": "vue/dist/vue"
  },
  "dependencies": {
    "pouchdb-browser": "^6.1.1",
    "socket.io": "1.7.2",
    "socket.io-client": "1.7.2",
    "threads": "^0.7.2",
    "vue": "^2.1.4",
    "worker-pouch": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.4.1",
    "cross-env": "^2.0.0",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.3.0",
    "uglify-js": "^2.5.0",
    "vue-i18n": "^4.7.1",
    "vue-socket.io": "^2.1.0",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0"
  }
}
